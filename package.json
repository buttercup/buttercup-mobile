{
  "name": "Buttercup",
  "version": "1.12.0",
  "private": true,
  "scripts": {
    "build:android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "clear:cache": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean --force && npm install && npm start -- --reset-cache",
    "format": "prettier --write '{{source,scripts,__tests__}/**/*.js,index.*.js,shim.js}'",
    "i18next:parse-lang": "i18next-scanner --config ./config/i18next-scanner.config.js 'source/**/*.{js,jsx}' && prettier ./locales/**/*.json --write",
    "nodeify": "rn-nodeify --install --hack",
    "patch:rnios": "sed -i '' 's/startsWith/includes/g' node_modules/react-native/local-cli/runIOS/findMatchingSimulator.js",
    "postinstall": "rn-nodeify --install --hack",
    "precommit": "lint-staged",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios --simulator='iPhone 11'",
    "start:ios:6s": "react-native run-ios --simulator='iPhone 6s'",
    "test": "run-s test:format test:react-native",
    "test:android": "./scripts/testAndroid.sh",
    "test:format": "prettier-check '{{source,scripts,__tests__}/**/*.js,index.*.js,shim.js}'",
    "test:react-native": "NODE_ENV=test jest --no-cache"
  },
  "lint-staged": {
    "{{source,scripts,__tests__}/**/*.js,index.*.js,shim.js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@buttercup/dropbox-client": "^0.4.0",
    "@buttercup/facades": "^1.0.0",
    "@buttercup/google-oauth2-client": "^0.2.1",
    "@buttercup/googledrive-client": "^0.8.0",
    "@perrymitchell/react-native-prompt": "^1.1.0",
    "@tradle/react-native-http": "^2.0.0",
    "assert": "^1.5.0",
    "asyncstorage-down": "^4.2.0",
