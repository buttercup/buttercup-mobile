{
  "name": "Buttercup",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "format": "prettier --write '{{source,scripts,__tests__}/**/*.js,index.*.js}'",
    "nodeify": "rn-nodeify --hack --install",
    "postinstall": "npm run nodeify",
    "precommit": "lint-staged",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios",
    "test": "jest",
    "test:android": "cd android && ./gradlew --project-cache-dir ../.gradlecache test"
  },
  "lint-staged": {
    "{{source,scripts,__tests__}/**/*.js,index.*.js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@buttercup/mobile-compat": "~0.6.1",
    "@yfuks/react-native-action-sheet": "0.0.3",
    "assert": "^1.4.1",
    "browserify-zlib": "^0.1.4",
    "buffer": "^3.6.0",
    "buttercup-web": "~0.41.0",
    "console-browserify": "^1.1.0",
    "constants-browserify": "0.0.1",
    "dns.js": "^1.0.1",
    "domain-browser": "^1.1.7",
    "dropbox": "^2.5.7",
    "events": "^1.1.1",
    "https-browserify": "0.0.1",
    "os-browserify": "^0.1.2",
    "path-browserify": "0.0.0",
    "process": "^0.11.10",
    "prop-types": "^15.5.10",
    "querystring-es3": "^0.2.1",
    "react": "16.0.0-alpha.6",
    "react-native": "0.44.0",
    "react-native-browser": "^0.6.2",
    "react-native-cell-components": "^0.2.1",
    "react-native-collapsible": "^0.8.0",
    "react-native-crypto": "^2.0.2",
    "react-native-elements": "^0.12.2",
    "react-native-exception-handler": "^1.1.0",
    "react-native-http": "github:tradle/react-native-http#834492d",
    "react-native-level-fs": "^3.0.0",
    "react-native-loading-spinner-overlay": "^0.5.0",
    "react-native-navbar": "^1.7.1",
    "react-native-notification": "^2.0.0",
    "react-native-prompt": "^1.0.0",
    "react-native-randombytes": "^2.2.0",
    "react-native-swipe-list-view": "^0.4.1",
    "react-native-swipeout": "^2.1.1",
    "react-native-tcp": "^2.0.4",
    "react-native-udp": "^1.2.9",
    "react-native-vector-icons": "^4.1.1",
    "react-navigation": "^1.0.0-beta.11",
    "react-redux": "^5.0.5",
    "readable-stream": "^1.0.33",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0",
    "stream-browserify": "^1.0.0",
    "string_decoder": "^0.10.31",
    "timers-browserify": "^1.4.2",
    "tty-browserify": "0.0.0",
    "url": "^0.10.3",
    "url-join": "^2.0.2",
    "util": "^0.10.3",
    "vm-browserify": "0.0.4"
  },
  "devDependencies": {
    "babel-jest": "20.0.3",
    "babel-plugin-jsx-control-statements": "^3.2.8",
    "babel-preset-react-native": "1.9.2",
    "husky": "^0.14.3",
    "jest": "~20.0.4",
    "jest-cli": "~20.0.4",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "react-test-renderer": "16.0.0-alpha.6",
    "rn-nodeify": "^7.0.1"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./scripts/jestInit.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-native-action-sheet)/"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "\\.history"
    ]
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "react-native-http",
    "https": "https-browserify",
    "os": "os-browserify",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify"
  },
  "browser": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "react-native-http",
    "https": "https-browserify",
    "os": "os-browserify",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify"
  }
}
