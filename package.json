    "process": "^0.11.10",
    "prop-types": "^15.7.2",
    "punycode": "^1.4.1",
    "querystring-es3": "^0.2.1",
    "ramda": "^0.26.1",
    "react": "16.9.0",
    "react-i18next": "9.0.10",
    "react-native": "0.61.5",
    "react-native-action-sheet": "^2.2.0",
