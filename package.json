{
  "name": "Buttercup",
  "version": "1.3.0",
  "private": true,
  "scripts": {
    "build:android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "format": "prettier --write '{{source,scripts,__tests__}/**/*.js,index.*.js,shim.js}'",
    "nodeify": "rn-nodeify --install --hack",
    "postinstall": "npm run nodeify",
    "precommit": "lint-staged",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "start:android": "react-native run-android",
    "start:ios": "react-native run-ios",
    "test": "run-s test:format test:react-native",
    "test:android": "cd android && ./gradlew --project-cache-dir ../.gradlecache test",
    "test:format": "prettier-check '{{source,scripts,__tests__}/**/*.js,index.*.js,shim.js}'",
    "test:react-native": "jest --no-cache"
  },
  "lint-staged": {
    "{{source,scripts,__tests__}/**/*.js,index.*.js,shim.js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@buttercup/mobile-compat": "~0.7.0",
    "@perrymitchell/react-native-prompt": "^1.1.0",
    "@tradle/react-native-http": "^2.0.0",
    "@yfuks/react-native-action-sheet": "0.0.4",
    "assert": "^1.4.1",
    "base-64": "^0.1.0",
    "browserify-zlib": "^0.1.4",
    "buffer": "^3.6.0",
    "buttercup": "2.0.4",
    "console-browserify": "^1.1.0",
    "constants-browserify": "0.0.1",
    "dns.js": "^1.0.1",
    "domain-browser": "^1.1.7",
    "dropbox": "^2.5.7",
    "events": "^1.1.1",
    "https-browserify": "0.0.1",
    "os-browserify": "^0.1.2",
    "path-browserify": "0.0.0",
    "process": "^0.11.10",
    "prop-types": "^15.6.1",
    "querystring-es3": "^0.2.1",
    "react": "^16.3.0-alpha.1",
    "react-native": "^0.54.4",
    "react-native-browser": "^0.6.2",
    "react-native-cell-components": "^0.4.0",
    "react-native-collapsible": "^0.8.0",
    "react-native-crypto": "^2.0.2",
    "react-native-dropdownalert": "^3.4.0",
    "react-native-elements": "^0.19.0",
    "react-native-exception-handler": "^1.1.0",
    "react-native-keychain": "^3.0.0-rc.3",
    "react-native-level-fs": "^3.0.0",
    "react-native-loading-spinner-overlay": "^0.5.0",
    "react-native-navbar": "^1.7.1",
    "react-native-os": "^1.1.0",
    "react-native-randombytes": "^3.0.0",
    "react-native-storage": "^0.2.2",
    "react-native-swipe-list-view": "^0.4.1",
    "react-native-swipeout": "^2.1.1",
    "react-native-tcp": "^2.0.4",
    "react-native-touch-id": "^4.0.1",
    "react-native-udp": "^1.2.9",
    "react-native-vector-icons": "^4.1.1",
    "react-navigation": "^1.0.0-beta.30",
    "react-navigation-redux-helpers": "^1.0.3",
    "react-redux": "^5.0.5",
    "readable-stream": "1.0.33",
    "redux": "^3.6.0",
    "redux-actions": "^2.0.3",
    "redux-thunk": "^2.2.0",
    "stream-browserify": "^1.0.0",
    "string_decoder": "^0.10.31",
    "timers-browserify": "^1.4.2",
    "tty-browserify": "0.0.0",
    "url": "^0.10.3",
    "url-join": "^2.0.2",
    "util": "^0.10.3",
    "vm-browserify": "0.0.4"
  },
  "devDependencies": {
    "babel-jest": "^22.4.1",
    "babel-plugin-jsx-control-statements": "^3.2.8",
    "babel-preset-react-native": "^4.0.0",
    "husky": "^0.14.3",
    "jest": "^22.4.2",
    "jest-cli": "^22.4.2",
    "jest-fetch-mock": "^1.6.2",
    "lint-staged": "^4.2.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.7.4",
    "prettier-check": "^2.0.0",
    "react-test-renderer": "^16.3.0-alpha.1",
    "rn-nodeify": "^10.0.0"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "jsdom",
    "setupFiles": [
      "./scripts/jestInit.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-native-action-sheet)/"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "\\.history"
    ]
  },
  "react-native": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  },
  "browser": {
    "zlib": "browserify-zlib",
    "console": "console-browserify",
    "constants": "constants-browserify",
    "crypto": "react-native-crypto",
    "dns": "dns.js",
    "net": "react-native-tcp",
    "domain": "domain-browser",
    "http": "@tradle/react-native-http",
    "https": "https-browserify",
    "os": "react-native-os",
    "path": "path-browserify",
    "querystring": "querystring-es3",
    "fs": "react-native-level-fs",
    "_stream_transform": "readable-stream/transform",
    "_stream_readable": "readable-stream/readable",
    "_stream_writable": "readable-stream/writable",
    "_stream_duplex": "readable-stream/duplex",
    "_stream_passthrough": "readable-stream/passthrough",
    "dgram": "react-native-udp",
    "stream": "stream-browserify",
    "timers": "timers-browserify",
    "tty": "tty-browserify",
    "vm": "vm-browserify",
    "tls": false
  }
}
